{
    "name": "netlogix/varnish-allowcookie",
    "description": "Set 'X-Allow-Cookie' header for configured request patterns.",
    "type": "neos-package",
    "require": {
        "php": "^7.4 || ~8.0 || ~8.1",
        "neos/flow": "^7.3 || ^8.0"
    },
    "require-dev": {
        "guzzlehttp/psr7": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Netlogix\\Varnish\\AllowCookie\\": "Classes/"
        }
    },
    "extra": {
        "neos": {
            "package-key": "Netlogix.Varnish.AllowCookie"
        }
    }
}
